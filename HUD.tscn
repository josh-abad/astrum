void fragment() {
vec2 uv = SCREEN_UV;

vec2 PixelSize = SCREEN_PIXEL_SIZE;
vec4 r_val = texture(SCREEN_TEXTURE, uv + vec2(r_offset_x, r_offset_y));
vec4 g_val = texture(SCREEN_TEXTURE, uv + vec2(g_offset_x, g_offset_y));
vec4 b_val = texture(SCREEN_TEXTURE, uv + vec2(b_offset_x, b_offset_y));

vec3 Chromatic_Aberration = vec3(r_val.r, g_val.g, b_val.b);

COLOR.rgb = Chromatic_Aberration;
// COLOR.rgb= textureLod(SCREEN_TEXTURE,uv,0.0).rgb;
}"

